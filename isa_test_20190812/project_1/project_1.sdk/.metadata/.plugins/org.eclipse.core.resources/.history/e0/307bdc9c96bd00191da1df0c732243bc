/*
 * Empty C++ Application
 */

#include "isa_hw.h"

#define READ  (0x02)
#define WRITE (0x01)

int main( int argc, char *argv[])
{
	int wdata_array0[33] = {
			0,  0, 0, 0, 0, 0, 0, 0, 0x83F,
			0x1, 0xF19, 0x101, 0xD780, 0x9E30, 0, 0x9F40,
			0, 0x2710, 0, 0, 0, 0, 0xE000, 0x200,
			0x40, 0, 0, 0, 0, 0, 0, 0, 0xF000
	};

	int rdata;
    int wdata;

    rdata = ISA_FPGA(READ, 0xEB00, wdata);
    if(rdata == 0xFFFF)
    	printf("[In] EB00 + DVN-4 = FFFF OK\n");
    else
    	printf("[In] EB00 + DVN-4 = FFFF ERROR: Read 0x%x\n", rdata);


    rdata = ISA_FPGA(READ, 0xE846, wdata);
    printf("RDATA = 0x%x\n", rdata);

    return 0;
}

