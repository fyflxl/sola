/*
 * Empty C++ Application
 */

#include "isa_hw.h"

#define READ  (0x02)
#define WRITE (0x01)

int main( int argc, char *argv[])
{
	int rdata;
    int wdata;

    rdata = ISA_FPGA(READ, EB00, wdata);
    if(rdata == 0xFFFF)
    	printf("[In] EB00 + DVN-4 = FFFF OK\n");
    else
    	printf("[In] EB00 + DVN-4 = FFFF ERROR: Read 0x%x\n", rdata);

    rdata = ISA_FPGA(READ, 0xE846, wdata);
    printf("RDATA = 0x%x\n", rdata);

    return 0;
}

